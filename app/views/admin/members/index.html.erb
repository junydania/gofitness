        
        <div class="page-wrapper">
                    <!-- ============================================================== -->
                    <!-- Bread crumb and right sidebar toggle -->
                    <!-- ============================================================== -->
                    <div class="row page-titles">
                        <div class="col-md-5 align-self-center">
                            <h3 class="text-themecolor">Gym Members</h3>
                        </div>
                        <div class="col-md-7 align-self-center">
                            <ol class="breadcrumb">
                                <li class="breadcrumb-item"><a href="javascript:void(0)">Home</a></li>
                                <li class="breadcrumb-item">Member</li>
                                <li class="breadcrumb-item active">All Members</li>
                            </ol>
                        </div>
                        <div>
                            <button class="right-side-toggle waves-effect waves-light btn-inverse btn btn-circle btn-sm pull-right m-l-10"><i class="ti-settings text-white"></i></button>
                        </div>
                    </div>
                    <!-- ============================================================== -->
                    <!-- End Bread crumb and right sidebar toggle -->
                    <!-- ============================================================== -->
                    <!-- ============================================================== -->
                    <!-- Container fluid  -->
                    <!-- ============================================================== -->
                    <div class="container-fluid">
                        <!-- ============================================================== -->
                        <!-- Start Page Content -->
                        <!-- ============================================================== -->
                        <div class="row">
                            <div class="col-12">
                                <div class="card">
                                    <div class="card-body">
                                        <h4 class="card-title">Member List</h4>
                                        <h6 class="card-subtitle"></h6>
                                        <div class="table-responsive">
                                            <table class="table m-t-30 table-hover contact-list" data-page-size="10">
                                                <thead>
                                                    <tr>
                                                        <th>ID</th>
                                                        <th>Customer Code
                                                        <th>Full Name</th>
                                                        <th>Membership Plan </th>
                                                        <th>Status</th>
                                                        <th>Expiry Date</th>
                                                        <th></th>
                                                        <th></th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                <% @members.each do |member| %>
                                                    <tr>
                                                        <td><%= member.id %></td>
                                                        <td><%= member.customer_code %></td>
                                                        <td>
                                                            <%= image_tag( "#{member.image_url}", alt: "member", size:'40', class:"img-circle" ) %><%= link_to "#{member.fullname}" %>
                                                        </td>
                                                        <td><span><%= member.subscription_plan.plan_name %><span></td>
                                                        <% if member.account_detail.member_status == "active" %>
                                                            <td><span class="label label-success"><%= member.account_detail.member_status.titleize %></span> </td>
                                                        <% else %>
                                                            <td><span class="label label-danger"><%= member.account_detail.member_status.titleize %></span> </td>
                                                        <% end %>
                                                        <td><%= member.account_detail.expiry_date %></td>
                                                        <td>
                                                            <%= link_to "View Profile", member_profile_path(member), class:"btn btn-info btn-rounded" %>
                                                        </td>
                                                    </tr>
                                                <% end %>
                                                </tbody>
                                                <tfoot>
                                                    <tr>
                                                        <td colspan="2">
                                                            <%= link_to "Add New Member", new_member_registration_path, class:"btn btn-info btn-rounded" %>                                                   
                                                        </td>                                               
                                                                <!-- /.modal-content -->
                                                            </div>
                                                            <!-- /.modal-dialog -->
                                                        </div>
                                                        <td colspan="7">
                                                            <div class="text-right">
                                                                <ul class="pagination"> </ul>
                                                            </div>
                                                        </td>
                                                    </tr>
                                                </tfoot>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>