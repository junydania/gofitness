<div class="page-wrapper">
    <!-- ============================================================== -->
    <!-- Bread crumb and right sidebar toggle -->
    <!-- ============================================================== -->
    <div class="row page-titles">
        <div class="col-md-5 align-self-center">
            <h3 class="text-themecolor">Manage All Members</h3>
        </div>
        <div class="col-md-7 align-self-center">
            <ol class="breadcrumb">
                <li class="breadcrumb-item"><a href="javascript:void(0)">Home</a></li>
                <li class="breadcrumb-item">Members</li>
                <li class="breadcrumb-item active">Renew Membership</li>
            </ol>
        </div>
        <div class="">
            <button class="right-side-toggle waves-effect waves-light btn-inverse btn btn-circle btn-sm pull-right m-l-10"><i class="ti-settings text-white"></i></button>
        </div>
    </div>


    <div class="container-fluid">
        <!-- ============================================================== -->
        <!-- Start Page Content -->
        <!-- ============================================================== -->
        <div
        <div class="row">
            <div class="col-12">
                <% flash.each do |key, value| %>
                    <div class="<%= flash_class(key) %>">
                            <%= value %>
                    </div>
                <% end %>
                </br>
                <div class="card">
                    
                    <div class form-group id="paystack-renew-success">
                    </div>

                    <div class="card-body">
                         <button id="pause-members-button" class="btn btn-success" href="#">Pause All Members</button>
                         <button id="unpause-members-button" class="btn btn-info" href="#"> Unpause All Members</button>
                    </div>

                </div>
                <div class="card remove-back-button">
                    <div class="card-body">
                        <%= link_to "Back", members_path, class: 'btn btn-danger m-r-10'%>
                    </div>
                </div>

                <div class="card hide" id="">

                </div>
            </div>

        </div>
        <div class="loader hide modal"></div>
    </div>
</div>
