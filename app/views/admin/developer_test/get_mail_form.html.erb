        <div class="page-wrapper">
            <!-- ============================================================== -->
            <!-- Bread crumb and right sidebar toggle -->
            <!-- ============================================================== -->
            <div class="row page-titles">
                <div class="col-md-5 align-self-center">
                    <h3 class="text-themecolor">Developer Test</h3>
                </div>
                <div class="col-md-7 align-self-center">
                    <ol class="breadcrumb">
                        <li class="breadcrumb-item"><a href="javascript:void(0)">Home</a></li>
                        <li class="breadcrumb-item">Developer</li>
                        <li class="breadcrumb-item active">Mail Test</li>
                    </ol>
                </div>
                <div class="">
                    <button class="right-side-toggle waves-effect waves-light btn-inverse btn btn-circle btn-sm pull-right m-l-10"><i class="ti-settings text-white"></i></button>
                </div>
            </div>


            <div class="container-fluid">
                <!-- ============================================================== -->
                <!-- Start Page Content -->
                <!-- ============================================================== -->
                <div>
                <div class="row">
                    <div class="col-12">
                        <div class="card">
                            <%= flash[:notice] %>
                        </div>
                        <br/>
                        <div class="card">
                            <div class="card-body">
                                <h4 class="card-title">Mailjet Test</h4>
                                <%= form_for(@user, url: { controller: :developer_test, action: :send_mail },  html: {class: 'form-group m-t-40' }) do |f| %>
                                    <div class="form-group">
                                        <%= f.label :first_name, 'First Name' %>
                                        <%= f.text_field :first_name, class: 'form-control form-control-line' %>
                                    </div>
                                     <div class="form-group">
                                        <%= f.label :last_name, 'Last Name' %>
                                        <%= f.text_field :last_name, class: 'form-control form-control-line' %>
                                    </div>
                                     <div class="form-group">
                                        <%= f.label :email, 'Email' %>
                                        <%= f.email_field :email, class: 'form-control form-control-line' %>
                                    </div>
                                    <div>
                                        <%= f.submit 'Test', class: 'btn btn-success m-r-10' %>
                                        <%= link_to 'Back', members_path, class: 'btn btn-danger m-r-10' %>
                                    </div>
                                <% end %>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
