        <div class="page-wrapper">
            <!-- ============================================================== -->
            <!-- Bread crumb and right sidebar toggle -->
            <!-- ============================================================== -->
            <div class="row page-titles">
                <div class="col-md-5 align-self-center">
                    <h3 class="text-themecolor">Staff</h3>
                </div>
                <div class="col-md-7 align-self-center">
                    <ol class="breadcrumb">
                        <li class="breadcrumb-item"><a href="javascript:void(0)">Home</a></li>
                        <li class="breadcrumb-item">Staff</li>
                        <li class="breadcrumb-item active">Edit Profile</li>
                    </ol>
                </div>
                <div class="">
                    <button class="right-side-toggle waves-effect waves-light btn-inverse btn btn-circle btn-sm pull-right m-l-10"><i class="ti-settings text-white"></i></button>
                </div>
            </div>


            <div class="container-fluid">
                <!-- ============================================================== -->
                <!-- Start Page Content -->
                <!-- ============================================================== -->
                <div
                <div class="row">
                    <div class="col-12">
                        <div class="card">
                            <%= flash[:notice] %>
                        </div>
                        </br>
                        <div class="card">
                            <div class="card-body">
                                <h4 class="card-title">Edit Profile</h4>
                                <%= form_for(resource, as: resource_name, url: user_registration_path(resource_name), html: {class: 'form-material m-t-40' }) do |f| %>
                                    <div class="form-group">
                                        <%= f.label :first_name, 'First Name' %>
                                        <%= f.text_field :first_name, class: 'form-control form-control-line' %>
                                    </div>
                                     <div class="form-group">
                                        <%= f.label :last_name, 'Last Name' %>
                                        <%= f.text_field :last_name, class: 'form-control form-control-line' %>
                                    </div>
                                    <div class="form-group">
                                        <%= f.label :email, 'Email' %>
                                        <%= f.email_field :email, autofocus: true, class: 'form-control', id:'example-email2' %>
                                    </div>
                                    <div class="form-group">
                                         <%= f.label :password, 'Password' %>
                                        <%= f.password_field :password,  class: "form-control", autocomplete: "off" %>
                                    </div>
                                    <div class="form-group">
                                         <%= f.label :password_confirmation, "Password Confirmation" %>
                                        <%= f.password_field :password_confirmation,  class: "form-control", autocomplete: "off" %>
                                    </div>
                                     <div class="form-group">
                                         <%= f.label :current_password, "Current Password" %>
                                        <%= f.password_field :current_password,  class: "form-control", autocomplete: "off" %>
                                    </div>
                                    <div>
                                        <%= f.submit 'Update', class: 'btn btn-danger m-r-10' %>
                                    </div>
                                <% end %>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
