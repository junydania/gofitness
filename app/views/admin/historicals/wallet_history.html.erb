        
        <div class="page-wrapper">
                    <!-- ============================================================== -->
                    <!-- Bread crumb and right sidebar toggle -->
                    <!-- ============================================================== -->
                    <div class="row page-titles">
                        <div class="col-md-5 align-self-center">
                            <h3 class="text-themecolor">Gym Members</h3>
                        </div>
                        <div class="col-md-7 align-self-center">
                            <ol class="breadcrumb">
                                <li class="breadcrumb-item"><a href="javascript:void(0)">Home</a></li>
                                <li class="breadcrumb-item">Member</li>
                                <li class="breadcrumb-item active">Wallet History</li>
                            </ol>
                        </div>
                        <div>
                            <button class="right-side-toggle waves-effect waves-light btn-inverse btn btn-circle btn-sm pull-right m-l-10"><i class="ti-settings text-white"></i></button>
                        </div>
                    </div>
                    <!-- ============================================================== -->
                    <!-- End Bread crumb and right sidebar toggle -->
                    <!-- ============================================================== -->
                    <!-- ============================================================== -->
                    <!-- Container fluid  -->
                    <!-- ============================================================== -->
                    <div class="container-fluid">
                        <!-- ============================================================== -->
                        <!-- Start Page Content -->
                        <!-- ============================================================== -->                        
                        <div class="row">
                            <div class="col-12">
                                <div class="card">
                                    <div class="card-body">
                                        <h4 class="card-title"><%= @member.first_name %>'s Wallet History</h4>
                                        <h6 class="card-subtitle"></h6>
                                        <div class="table-responsive">
                                            <table class="table m-t-30 table-hover contact-list" data-page-size="10">
                                                <thead>
                                                    <tr>
                                                        <th>ID</th>
                                                        <th>Amount Paid In </th>
                                                        <th>Previous Balance</th>
                                                        <th>Amount Used</th>
                                                        <th>Wallet Balance</th>
                                                        <th>Wallet Fund Date</th>
                                                        <th>Staff</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                <% @wallet_history_activities.each do |wallet_history| %>
                                                    <tr>
                                                        <td><%= wallet_history.id %></td>
                                                        <td>₦ <%= wallet_history.amount_paid_in %></td>
                                                        <td><span>₦ <%= wallet_history.wallet_previous_balance %><span></td> 
                                                        <td>₦ <%= wallet_history.amount_used %></td>  
                                                        <td><span class="label label-success">₦<%= wallet_history.wallet_new_balance %></span> </td>
                                                        <td><%= wallet_history.created_at %></td>
                                                        <td><%= wallet_history.processed_by %></td>
                                                    </tr>
                                                <% end %>
                                                </tbody>
                                            </table>
                                            <tfoot>
                                                <td colspan="2">
                                                    <%= link_to "Back", member_profile_path(@member), class:"btn btn-info btn-rounded" %>                                                   
                                                </td>                                               
                                            </tfoot>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>